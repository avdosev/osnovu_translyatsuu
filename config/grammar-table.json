{
    "<больше>":       {">" : [">"]}, 
    "<меньше>":       {"<" : ["<"]}, 
    "<равно>":        {"=" : ["="]}, 
    "<больше-равно>": {">=" : [">="]}, 
    "<меньше-равно>": {"<=" : ["<="]}, 
    "<неравно>":      {"/=" : ["/="]}, 
    "<умножить>":     {"*" : ["*"]}, 
    "<делить>":       {"/" : ["/"]}, 
    "<цел-делить>":   {"//" : ["//"]}, 
    "<остаток>":      {"\\\\" : ["\\\\"]},
    "<присвоить>":    {":=" : [":="]}, 
    "<плюс>":         {"+" : ["+"]}, 
    "<минус>":        {"-" : ["-"]}, 
    "<and>":          {"and" : ["and"]}, 
    "<or>":           {"or" : ["or"]},
    "<откр. скобка>": {"(" : ["("]},
    "<закр. скобка>": {")" : [")"]},
    "<запятая>":      {"," : [","]},
    "<value>" : {
        "id" : ["<value_id>"],
        "num" : ["<value_num>"]
    },
    "<value_num>" : {
        "num" : ["num"],
        
        "<<put_in_tree>>" : true
    },
    "<value_id>" : {
        "id" : ["id"],
        
        "<<put_in_tree>>" : true
    }, 


    "<программа>" : {
        "class" : ["class", "id", "create", "id", "feature", "<блоки функций>", "end"]
    },
    "<блок объявления переменных>":{
        "local" : ["local", "<объявление переменных>"],
        "do" : []
    },
    "<объявление переменных>": {
        "id" : ["<блок_id>", ":", "<тип>", ";", "<объявление переменных с запятой>"],
        "do" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<объявление переменных с запятой>" : {
        "id" : ["<объявление переменных>"],
        "do" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<блок_id>": {
        "id" : ["<value_id>", "<блок_id>"],
        "," : ["<запятая>", "<value_id>", "<блок_id>"],
        ":" : [],
        ")" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<тип>": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY", "[", "<тип>", "]"]
    },
    "<блоки функций>": {
        "id" : ["<блок функции>", "<блоки функций>"],
        "end" : [],
        
        "<<dont_recursion_reversal>>" : true
    },
    "<блок функции>": {
        "id" : ["<value>", "<аргументы функции>", "<блок объявления переменных>", "<тело функции>"]
    },
    "<аргументы функции>": {
        "(" : ["(", "<аргумент функции>", "<еще аргументы функции>" , ")"],
        "local" : [],
        "do" : []
    },
    "<аргумент функции>" : {
        "id" : ["<блок_id>", ":", "<тип>"],
        ")" : []
    },
    "<еще аргументы функции>" : {
        ";" : [";", "<аргумент функции>", "<еще аргументы функции>"],
        ")" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<тело функции>": {
        "do" : ["<блок>"]
    },
    "<блок>": {
        "do": ["do", "<тело>", "end"]
    },
    "<тело>": {
        "id": ["<действие>", "<тело>"],
        "num": ["<действие>", "<тело>"],
        "if": ["<блок если>", "<тело>"],
        "from": ["<блок цикла c преддействием>", "<тело>"],
        "until": ["<блок цикла с предусловием>", "<тело>"],
        "loop": ["<блок цикла с предусловием>", "<тело>"],
        ";": [";", "<тело>"],
        "end": [],
        "else": [],
        
        "<<dont_push_if_empty>>" : true
    },
    "<блок если>": {
        "if" : ["if", "<<value>>", "then", "<тело>", "<блок иначе>", "end"],
        "<<value>>" : ["<выражение>"]
    },
    "<блок иначе>": {
        "else":["<else>", "<тело>"],
        "end":[]
    },
    "<else>" : {"else" : ["else"]},
    "<блок цикла c преддействием>" : {
        "from": ["from", "<действие>", "<блок цикла>"],
        "until" : ["<блок цикла с предусловием>"],
        "loop" : ["<блок цикла>"]
    },
    "<блок цикла с предусловием>" : {
        "until": ["until", "<<value>>", "<блок цикла>"],
        "loop" : ["<блок цикла>"],
        "<<value>>" : ["<выражение>", ";"]
    },
    "<блок цикла>": {
        "loop": ["loop", "<тело>", "end"]
    },
    "<действие>": { 
        "id" : ["<выражение>", ";"],
        "num": ["<выражение>", ";"]  
    }, 
    "<аргументы вызываемой функции>": { 
        ")": [], 
        "id": ["<выражение>", "<аргументы вызываемой функции с запятой>"],
        "num" : ["<выражение>", "<аргументы вызываемой функции с запятой>"]
    },
    "<аргументы вызываемой функции с запятой>": {
        "," : ["<запятая>", "<выражение>", "<аргументы вызываемой функции с запятой>"],
        ")" : []
    },
    
    "<пр. выр.>": { 
        "+": ["<последовательность слагаемых>"], 
        "-": ["<последовательность слагаемых>"], 
        "(": ["(","<пр. выр.>",")"], 
        "id": ["<слагаемое>", "<последовательность слагаемых>"],
        "num" : ["<слагаемое>", "<последовательность слагаемых>"] 
    },
    "<выражение>" : {
        "(": ["<откр. скобка>","<выражение>","<закр. скобка>", "<часть выражения>"],
        "id": ["<value_id>", "<аргументы для id>", "<часть выражения>"],
        "num": ["<value_num>", "<часть выражения>"],
        "-" : ["<игра с минусиками>", "<выражение>"],
        "+" : ["<игра с минусиками>", "<выражение>"],
        "<<EMPTY PRODUCTIONS>>" : [")"]
    },
    "<игра с минусиками>" : {
        "-" :["<минус>",  "<игра с минусиками>"],
        "+" :["<плюс>", "<игра с минусиками>"],
        "<<EMPTY PRODUCTIONS>>" : ["(", "id", "num"]
    },
    "<часть выражения>": {
        "id" : ["<выражение>"],
        "const": ["<выражение>"],
        ">" : ["<больше>",  "<выражение>", "<часть выражения>"],
        "<" : ["<меньше>",  "<выражение>", "<часть выражения>"],
        "=":["<равно>",   "<выражение>", "<часть выражения>"], 
        ">=":["<больше-равно>", "<выражение>", "<часть выражения>"], 
        "<=":["<меньше-равно>", "<выражение>", "<часть выражения>"], 
        "/=":["<неравно>", "<выражение>", "<часть выражения>"],
        "+" : ["<плюс>",  "<выражение>", "<часть выражения>"],
        "-" : ["<минус>",  "<выражение>", "<часть выражения>"],
        "*" : ["<умножить>",  "<выражение>", "<часть выражения>"],
        "/" : ["<делить>",  "<выражение>", "<часть выражения>"],
        "\\\\" : ["<остаток>",  "<выражение>", "<часть выражения>"],
        "//" : ["<цел-делить>",  "<выражение>", "<часть выражения>"],
        "and" : ["<and>",  "<выражение>", "<часть выражения>"],
        "or" : ["<or>",  "<выражение>", "<часть выражения>"],
        ":=" : ["<присвоить>", "<выражение>", "<часть выражения>"],
        "<<EMPTY PRODUCTIONS>>": [";", ")", "then", "end", ","]
    },
    "<аргументы для id>": {
        "(": ["(","<аргументы вызываемой функции>",")"],
        "<<EMPTY PRODUCTIONS>>": ["+","-",":=", ">","=", "<", ">=", "/=", ";", "<=", "/", "\\\\", "and", "or", "*", "/", ")"]
    }
    
}