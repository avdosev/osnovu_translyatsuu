{
    "<программа>" : {
        "class" : ["class", "id", "create", "id", "feature", "<блоки функций>", "end"]
    },
    "<блок_id>": {
        "id" : ["<value_id>", "<блок_id>"],
        "," : [",", "<value_id>", "<блок_id>"],
        ":" : [],
        "<<dont_push_if_empty>>" : true
    },
    "<блок объявления переменных>":{
        "local" : ["local", "<объявление переменных>"],
        "do" : []
    },
    "<объявление переменных>": {
        "id" : ["<блок_id>", ":", "<тип>", "<объявление переменных>"],
        "do" : []
    },
    "<тип>": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY"]
    },
    "<блоки функций>": {
        "id" : ["<блок функции>", "<блоки функций>"],
        "end" : [],
        
        "<<dont_recursion_reversal>>" : true
    },
    "<блок функции>": {
        "id" : ["<value>", "<аргументы функции>", "<блок объявления переменных>", "<тело функции>"]
    },
    "<аргументы функции>": {
        "(" : ["(", "<объявление переменных>" , ")"],
        "local" : [],
        "do" : []
    },
    "<тело функции>": {
        "do" : ["<блок>"]
    },
    "<блок>": {
        "do": ["do", "<тело>", "end"]
    },
    "<тело>": {
        "id": ["<действие>", "<тело>"],
        "num": ["<действие>", "<тело>"],
        "if": ["<блок если>", "<тело>"],
        "from": ["<блок цикла c преддействием>", "<тело>"],
        "until": ["<блок цикла с предусловием>", "<тело>"],
        "loop": ["<блок цикла с предусловием>", "<тело>"],
        ";": [";", "<тело>"],
        "end": [],
        "else": [],
        
        "<<dont_push_if_empty>>" : true
    }, 
    "<блок если>": {
        "if" : ["if", "<предусловие>", "then", "<тело>", "<блок иначе>", "end"]
    },
    "<блок иначе>": {
        "else":["<else>", "<тело>"],
        "end":[]
    },
    "<else>" : {"else" : ["else"]},
    "<блок цикла c преддействием>" : {
        "from": ["from", "<действие>", "<блок цикла>"],
        "until" : ["<блок цикла с предусловием>"],
        "loop" : ["<блок цикла>"]
    },
    "<блок цикла с предусловием>" : {
        "until": ["until", "<предусловие>", ";", "<brek>", "<блок цикла>"],
        "loop" : ["<блок цикла>"]
    },
    "<brek>" : {
        "loop" : []
    },
    "<предусловие>" : {
        "id" : ["<выражение>"],
        "num" : ["<выражение>"]
    },
    "<блок цикла>": {
        "loop": ["loop", "<тело>", "end"]
    },
    "<выражение>" : {
        "id": ["<пр. выр.>", "<сравн. с пр. выр.>"],
        "num" : ["<пр. выр.>", "<сравн. с пр. выр.>"]
    },
    "<действие>": { 
        "id" : ["<выражение>", ";"],
        "num": ["<выражение>", ";"]  
    }, 
    "<аргументы вызываемой функции>": { 
        ")": [], 
        "id": ["<выражение>", "<аргументы вызываемой функции с запятой>"],
        "num" : ["<выражение>", "<аргументы вызываемой функции с запятой>"]
    },
    "<аргументы вызываемой функции с запятой>": {
        "," : [",", "<выражение>", "<аргументы вызываемой функции с запятой>"],
        ")" : []
    },
    "<value>" : {
        "id" : ["<value_id>"],
        "num" : ["<value_num>"]
    },
    "<value_num>" : {
        "num" : ["num"],
        
        "<<put_in_tree>>" : true
    },
    "<value_id>" : {
        "id" : ["id"],
        
        "<<put_in_tree>>" : true
    },
    "<больше>":       {">" : [">"]}, 
    "<меньше>":       {"<" : ["<"]}, 
    "<равно>":        {"=" : ["="]}, 
    "<больше-равно>": {">=" : [">="]}, 
    "<меньше-равно>": {"<=" : ["<="]}, 
    "<неравно>":      {"/=" : ["/="]}, 
    "<умножить>":     {"*" : ["*"]}, 
    "<делить>":       {"/" : ["/"]}, 
    "<цел-делить>":   {"//" : ["//"]}, 
    "<остаток>":      {"\\\\" : ["\\\\"]},
    "<присвоить>":    {":=" : [":="]}, 
    "<плюс>":         {"+" : ["+"]}, 
    "<минус>":        {"-" : ["-"]}, 
    "<and>":          {"and" : ["and"]}, 
    "<or>":           {"or" : ["or"]},

    "<сравн. с пр. выр.>": { 
        ">" :["<больше>",  "<пр. выр.>"], 
        "<" :["<меньше>",  "<пр. выр.>"], 
        "=":["<равно>",   "<пр. выр.>"], 
        ">=":["<больше-равно>", "<пр. выр.>"], 
        "<=":["<меньше-равно>", "<пр. выр.>"], 
        "/=":["<неравно>", "<пр. выр.>"],
        "<<EMPTY PRODUCTIONS>>": [";", ")", ","] 
    }, 
    "<пр. выр.>": { 
        "+": ["<последовательность слагаемых>"], 
        "-": ["<последовательность слагаемых>"], 
        "(": ["(","<пр. выр.>",")"], 
        "id": ["<слагаемое>", "<последовательность слагаемых>"],
        "num" : ["<слагаемое>", "<последовательность слагаемых>"] 
    }, 
    "<последовательность слагаемых>": { 
        "+": ["<плюс>", "<слагаемое>", "<последовательность слагаемых>"], 
        "-": ["<минус>", "<слагаемое>", "<последовательность слагаемых>"], 
        "or": ["<or>", "<слагаемое>", "<последовательность слагаемых>"],
        "<<EMPTY PRODUCTIONS>>": [",",")", ";", "<", ">","=", "/=", ">=", "<=", "then"]
    }, 
    "<слагаемое>": { 
        "(": ["(","<пр. выр.>",")", "<последовательность множителей>"], 
        "id": ["<множитель>", "<последовательность множителей>"],
        "num": ["<множитель>", "<последовательность множителей>"] 
    }, 
    "<последовательность множителей>": { 
        "*": ["<умножить>", "<множитель>", "<последовательность множителей>"], 
        "/": ["<делить>", "<множитель>", "<последовательность множителей>"],
        "//": ["<цел-делить>", "<множитель>", "<последовательность множителей>"], 
        "\\\\": ["<остаток>", "<множитель>", "<последовательность множителей>"], 
        "<<EMPTY PRODUCTIONS>>": ["+", "-", "or", ";", ")", "<", ">",":=","=", "/=", ">=", "<=", "then", ","]
    }, 
    "<множитель>": { 
        "id": ["<value_id>", "<аргументы для id>"],
        "num": ["<value_num>"]
    },
    "<аргументы для id>":{
        "(": ["(","<аргументы вызываемой функции>",")"],
        ":=": ["<присвоить>", "<выражение>"],
        "<<EMPTY PRODUCTIONS>>": ["+","-",":=", ">","=", "<", ">=", "/=", ";", "<=", "/", "\\\\", "and", "or", "*", "/", ")"]
    }
    
}