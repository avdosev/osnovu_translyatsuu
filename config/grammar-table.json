{
    "<программа>" : {
        "class" : ["class", "id", "create", "id", "feature", "<блоки функций>", "end"]
    },
    "<блок_id>": {
        "id" : ["id", "<блок_id>"],
        "," : [",", "id", "<блок_id>"],
        ":" : []
    },
    "<блок объявления переменных>":{
        "local" : ["local", "<объявление переменных>"],
        "do" : []
    },
    "<объявление переменных>": {
        "id" : ["<блок_id>", ":", "<тип>", "<объявление переменных>"],
        "do" : []
    },
    "<тип>": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY"]
    },
    "<блоки функций>": {
        "id" : ["<блок функции>", "<блоки функций>"],
        "end" : []
    },
    "<блок функции>": {
        "id" : ["id", "<аргументы функции>", "<блок объявления переменных>", "<тело функции>"]
    },
    "<аргументы функции>": {
        "(" : ["(", "<объявление переменных>" , ")"],
        "local" : [],
        "do" : []
    },
    "<тело функции>": {
        "do" : ["<блок>"]
    },
    "<блок>": {
        "do": ["do", "<тело>", "end"]
    },
    "<тело>": {
        "id": ["<действие>", "<тело>"],
        "if": ["<блок если>", "<тело>"],
        "from": ["<блок цикла>", "<тело>"],
        "until": ["<блок цикла>", "<тело>"],
        "loop": ["<блок цикла>", "<тело>"],
        "end": [],
        "not_razvorot_in_rekursiya": true
    }, 
    "<блок если>": {
        "if" : ["if", "<действие>", "then", "<тело>", "<блок иначе>", "end"]
    },
    "<блок иначе>": {
        "else":["else", "<тело>"],
        "end":[]
    },
    "<блок цикла>": {
        "from": ["from", "<действие>", "<блок цикла>"],
        "until": ["until", "<действие>", "<блок цикла>"],
        "loop": ["loop", "<тело>", "end"]
    },

    "<действие>": { 
        "id" : ["id","<действие с ид-ом>", ";"] 
    }, 
    "<действие с ид-ом>": { 
        ":=": [":=", "<правостороннее выражение>"], 
        "(": ["(","<аргументы вызываемой функции>",")"] 
    }, 
    "<аргументы вызываемой функции>": { 
        ")": [], 
        "id": ["id", "<аргументы вызываемой функции с запятой>"],
        "num" : ["num", "<аргументы вызываемой функции с запятой>"] 
    }, 
    "<аргументы вызываемой функции с запятой>": { 
        ",":[",", "<аргументы вызываемой функции>"], 
        ")": [] 
    }, 
    "<правостороннее выражение>": { 
        "id": ["<простое выражение>","<сравнение с простым выражением>"] 
    }, 
    "<сравнение с простым выражением>": { 
        ">" :[">", "<простое выражение>"], 
        "<" :["<", "<простое выражение>"], 
        "=":["=", "<простое выражение>"], 
        ">=":[">=", "<простое выражение>"], 
        "<=":["<=", "<простое выражение>"], 
        "/=":["/=", "<простое выражение>"], 
        ";":[] 
    }, 
    "<простое выражение>": { 
        "+": ["<последовательность слагаемых>"], 
        "-": ["<последовательность слагаемых>"], 
        "(": ["(","<простое выражение>",")"], 
        "id": ["<слагаемое>", "<последовательность слагаемых>"] 
    }, 
    "<последовательность слагаемых>": { 
        "+": ["+", "<слагаемое>", "<последовательность слагаемых>"], 
        "-": ["-", "<слагаемое>", "<последовательность слагаемых>"], 
        "or": ["or", "<слагаемое>", "<последовательность слагаемых>"], 
        ")": [] 
    }, 
    "<слагаемое>": { 
        "(": ["(","<простое выражение>",")", "<последовательность множителей>"], 
        "id": ["<множитель>", "<последовательность множителей>"] 
    }, 
    "<последовательность множителей>": { 
        "*": ["*", "<множитель>", "<последовательность множителей>"], 
        "/": ["/", "<множитель>", "<последовательность множителей>"], 
        "//": ["//", "<множитель>", "<последовательность множителей>"], 
        "\\\\": ["\\\\", "<множитель>", "<последовательность множителей>"], 
        "and": ["and", "<множитель>", "<последовательность множителей>"], 
        "+":[], 
        "-":[], 
        "or":[], 
        ";": [], 
        ")": [] 
    }, 
    "<множитель>": { 
        "not": ["not","<простое выражение>"], 
        "id": ["id"] 
    }
}