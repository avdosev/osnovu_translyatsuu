{
    "<программа>" : {
        "class" : ["class", "id", "create", "id", "feature", "<блоки функций>", "end"]
    },
    "<блок_id>": {
        "id" : ["<value_id>", "<блок_id>"],
        "," : [",", "<value_id>", "<блок_id>"],
        ":" : []
    },
    "<блок объявления переменных>":{
        "local" : ["local", "<объявление переменных>"],
        "do" : []
    },
    "<объявление переменных>": {
        "id" : ["<блок_id>", ":", "<тип>", "<объявление переменных>"],
        "do" : []
    },
    "<тип>": {
        "INTEGER" : ["INTEGER"],
        "REAL" : ["REAL"],
        "STRING" : ["STRING"],
        "ARRAY" : ["ARRAY"]
    },
    "<блоки функций>": {
        "id" : ["<блок функции>", "<блоки функций>"],
        "end" : [],
        
        "<<dont_recursion_reversal>>" : true
    },
    "<блок функции>": {
        "id" : ["<value>", "<аргументы функции>", "<блок объявления переменных>", "<тело функции>"]
    },
    "<аргументы функции>": {
        "(" : ["(", "<объявление переменных>" , ")"],
        "local" : [],
        "do" : []
    },
    "<тело функции>": {
        "do" : ["<блок>"]
    },
    "<блок>": {
        "do": ["do", "<тело>", "end"]
    },
    "<тело>": {
        "id": ["<действие>", "<тело>"],
        "if": ["<блок если>", "<тело>"],
        "from": ["<блок цикла>", "<тело>"],
        "until": ["<блок цикла>", "<тело>"],
        "loop": ["<блок цикла>", "<тело>"],
        "end": [],
        ";":[],

        "<<dont_recursion_reversal>>" : true
    }, 
    "<блок если>": {
        "if" : ["if", "<правостороннее выражение>", "then", "<тело>", "<блок иначе>", "end"]
    },
    "<блок иначе>": {
        "else":["else", "<тело>"],
        "end":[]
    },
    "<блок цикла>": {
        "from": ["from", "<действие>", "<блок цикла>"],
        "until": ["until", "<правостороннее выражение>", "<блок цикла>"],
        "loop": ["loop", "<тело>", "end"]
    },
    "<выражение>" : {
        "id" : ["<value>","<действие с ид-ом>"] 
    },
    "<действие>": { 
        "id" : ["<выражение>", ";"]
    }, 
    "<действие с ид-ом>": { 
        ":=": [":=", "<правостороннее выражение>"], 
        "(": ["(","<аргументы вызываемой функции>",")"],
        ";":[]
    }, 
    "<аргументы вызываемой функции>": { 
        ")": [], 
        "id": ["<value>", "<аргументы вызываемой функции с запятой>"],
        "num" : ["<value>", "<аргументы вызываемой функции с запятой>"]
    },
    "<аргументы вызываемой функции с запятой>": {
        "," : [",", "<value>", "<аргументы вызываемой функции с запятой>"],
        ")" : []
    },
    "<value>" : {
        "id" : ["<value_id>"],
        "num" : ["<value_num>"]
    },
    "<value_num>" : {
        "num" : ["num"],
        
        "<<put_in_tree>>" : true
    },
    "<value_id>" : {
        "id" : ["id"],
        
        "<<put_in_tree>>" : true
    },
    "<оператор>":{
        ">" :["<больше>"], 
        "<" :["<меньше>"], 
        "=":["<равно>"], 
        ">=":["<больше-равно>"], 
        "<=":["<меньше-равно>"], 
        "/=":["<неравно>"], 
        "*": ["<умножить>"], 
        "/": ["<делить>"], 
        "//": ["<цел-делить>"], 
        "\\\\": ["<остаток>"],
        ":=":["<присвоить>"], 
        "+":["<плюс>"], 
        "-":["<минус>"], 
        "and": ["<and>"], 
        "or":["<or>"]
    },
    "<правостороннее выражение>": { 
        "id": ["<простое выражение>","<сравнение с простым выражением>"],
        "num":["<простое выражение>","<сравнение с простым выражением>"] 
    }, 
    "<сравнение с простым выражением>": { 
        ">" :[">", "<простое выражение>"], 
        "<" :["<", "<простое выражение>"], 
        "=":["=", "<простое выражение>"], 
        ">=":[">=", "<простое выражение>"], 
        "<=":["<=", "<простое выражение>"], 
        "/=":["/=", "<простое выражение>"], 
        ";":[] 
    }, 
    "<простое выражение>": { 
        "+": ["<последовательность слагаемых>"], 
        "-": ["<последовательность слагаемых>"], 
        "(": ["(","<простое выражение>",")"], 
        "id": ["<слагаемое>", "<последовательность слагаемых>"],
        "num" : ["<слагаемое>", "<последовательность слагаемых>"] 
    }, 
    "<последовательность слагаемых>": { 
        "+": ["+", "<слагаемое>", "<последовательность слагаемых>"], 
        "-": ["-", "<слагаемое>", "<последовательность слагаемых>"], 
        "or": ["or", "<слагаемое>", "<последовательность слагаемых>"], 
        ")": [],
        ";":[] 
    }, 
    "<слагаемое>": { 
        "(": ["(","<простое выражение>",")", "<последовательность множителей>"], 
        "id": ["<множитель>", "<последовательность множителей>"],
        "num": ["<множитель>", "<последовательность множителей>"] 
    }, 
    "<последовательность множителей>": { 
        "*": ["*", "<множитель>", "<последовательность множителей>"], 
        "/": ["/", "<множитель>", "<последовательность множителей>"], 
        "//": ["//", "<множитель>", "<последовательность множителей>"], 
        "\\\\": ["\\\\", "<множитель>", "<последовательность множителей>"], 
        "and": ["and", "<множитель>", "<последовательность множителей>"], 
        "+":[], 
        "-":[], 
        "or":[], 
        ";": [], 
        ")": [] 
    }, 
    "<множитель>": { 
        "not": ["not","<простое выражение>"], 
        "id": ["<value>"],
        "num": ["<value>"]
    }
}